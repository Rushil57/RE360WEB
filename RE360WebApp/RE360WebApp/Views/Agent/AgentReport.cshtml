@{
    Layout = "~/Views/PartialView/Layout.cshtml";
}
@*<script data-require="angular.js@1.1.5" data-semver="1.1.5" src="http://code.angularjs.org/1.1.5/angular.min.js">
</script>
<link data-require="bootstrap-css@2.3.2" data-semver="2.3.2" rel="stylesheet"
      href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.3.2/css/bootstrap-combined.min.css" />
<script data-require="angular-ui-bootstrap@0.3.0" data-semver="0.3.0"
        src="http://angular-ui.github.io/bootstrap/ui-bootstrap-tpls-0.3.0.min.js">
</script>*@
<link href="~/css/stylereport.css" rel="stylesheet" />
<script src="~/js/AgentReport.js"></script>
<center>
    <div ng-app="MyAppAgentReport" ng-controller="AgentReportController" ng-init="init()" class="Dashboard" style="width: 744px; height: auto; padding-bottom: 41px; background: white; flex-direction: column; justify-content: flex-start; align-items: center; gap: 40px; display: inline-flex">
        <div class="TopNavigation">
            <div class="OfficialLogoRe360">
                <div class="Group">
                    <img src="~/images/logore360.png" style="height: 66px;width: 90px;" />
                </div>
            </div>
            <div class="TitleDashboard" style="padding-left: 20px; padding-right: 20px; justify-content: center; align-items: center; gap: 4px; display: flex">
                <div class="Dashboard" style="text-align: center; color: #001F49; font-size: 20px; font-family: sans-serif; font-weight: 900; line-height: 28px; word-wrap: break-word">DASHBOARD</div>
            </div>
        </div>
        <button ng-click="redirectRegi('')" class="ButtonCtas" style="width: 686px; padding: 16px; background: #001F49; box-shadow: 0px 8px 12px rgba(0, 0, 0, 0.04); border-radius: 8px; justify-content: center; align-items: center; gap: 12px; display: inline-flex">
            <div style="width: 24px; height: 24px; position: relative">
                <img src="~/images/plus.png" />
            </div>
            <div class="PrimaryDefault" style="text-align: center; color: white; font-size: 18px; font-family: sans-serif; font-weight: 700; line-height: 24px; word-wrap: break-word">Create Property Details</div>
        </button>
        <div class="ListOfClient" ng-if="AgentDetail.length>0">
            <div class="ClientList" ng-repeat="item in AgentDetail | orderBy:'FirstName'">
                @*ng-if="$index<10"*@
                <div ng-class="{'ListView':$index%2==0,'ListView2':$index%2!=0}">
                    <br />
                    <div class="UserDetails">
                        <div class="ClientDiv">
                            <div style="justify-content: flex-start; align-items: flex-start; gap: 4px; display: flex">
                                <div class="UserSolid">
                                    <img src="~/images/user.svg" style="width: 24px; height: 24px;" />
                                </div>
                                <div style="width: 236px; flex-direction: column; justify-content: center; align-items: flex-start; gap: 4px; display: inline-flex">
                                    <div class="CameronWilliamsonDavid" style="align-self: stretch; color: #000000; font-size: 16px; font-family: sans-serif; font-weight: 500; line-height: 24px; word-wrap: break-word">
                                        {{item.firstName}}
                                    </div>
                                </div>
                            </div>
                            <div style="justify-content: flex-start; align-items: flex-start; gap: 4px; display: flex">
                                <div class="UserSolid" style="width: 24px; height: 24px; position: relative">
                                    <img src="~/images/email_icon.svg" style="width: 24px; height: 24px;" />
                                </div>
                                <div class="CreateDate" style="width: 236px; flex-direction: column; justify-content: center; align-items: flex-start; gap: 4px; display: inline-flex">
                                    <div class="July20231000Pm" style="align-self: stretch; color: #000000; font-size: 16px; font-family: sans-serif; font-weight: 500; line-height: 24px; word-wrap: break-word">
                                        {{item.email}}
                                    </div>
                                </div>
                            </div>
                            <button ng-click="redirectRegi(item.agentID)" class="SharePdf" style="padding: 8px; background: #E6E9ED; border-radius: 4px; overflow: hidden; justify-content: center; align-items: center; gap: 9px; display: inline-flex;width: 95px;border: none;">
                                <div class="Share2" style="width: 16px; height: 16px; position: relative;">
                                    <img src="~/images/edit_notepad_icon.svg" style="width: 16px; height: 16px;" />
                                </div>
                                <div class="SharePdf" style="color: #000000; font-size: 16px; font-family: sans-serif; font-weight: 500; line-height: 16px; word-wrap: break-word">Edit</div>
                            </button>
                        </div>
                        <div class="ClientDiv">
                            <div style="justify-content: flex-start; align-items: flex-start; gap: 4px; display: flex">
                                <div class="UserSolid">
                                    <img src="~/images/companylogo.svg" style="width: 24px; height: 24px;" />
                                </div>
                                <div style="width: 236px; flex-direction: column; justify-content: center; align-items: flex-start; gap: 4px; display: inline-flex">
                                    <div class="CameronWilliamsonDavid" style="align-self: stretch; color: #878786; font-size: 16px; font-family: sans-serif; font-weight: 500; line-height: 24px; word-wrap: break-word">
                                        {{item.companyName}}
                                    </div>
                                </div>
                            </div>
                            <div style="justify-content: flex-start; align-items: flex-start; gap: 4px; display: flex">
                                <div class="UserSolid">
                                    <img src="~/images/officelogo.svg" style="width: 24px; height: 24px;" />
                                </div>
                                <div class="CreateDate" style="width: 236px; flex-direction: column; justify-content: center; align-items: flex-start; gap: 4px; display: inline-flex">
                                    <div class="July20231000Pm" style="align-self: stretch; color: #878786; font-size: 16px; font-family: sans-serif; font-weight: 500; line-height: 24px; word-wrap: break-word">
                                        {{item.offinceName}}
                                    </div>
                                </div>
                            </div>
                            <button ng-click="DeleteAgentByID(item.agentID)" class="deleteuser" style="padding-left: 10px; padding-right: 10px; padding-top: 8px; padding-bottom: 8px; background: #FCEBEC; border-radius: 4px; overflow: hidden; justify-content: center; align-items: center; gap: 9px; display: inline-flex;border: none;">
                                <div class="FilePdf" style="width: 16px; height: 16px; position: relative">
                                    <img src="~/images/red-delete.svg" style="width: 16px; height: 16px;" />
                                </div>
                                <div class="ViewPdf" style="color: #DC3545; font-size: 16px; font-family: sans-serif; font-weight: 500; line-height: 16px; word-wrap: break-word">Delete</div>
                            </button>
                        </div>
                    </div>
                </div>
            @*    <pagination ng-model="currentPage"
                            total-items="todos.length"
                            max-size="maxSize"
                            boundary-links="true">
                </pagination>*@

            </div>
            <div>
                @*<div class="pagination">
                    <a href="#">&laquo;</a>
                    <a href="#">1</a>
                    <a class="active" href="#">2</a>
                    <a href="#">3</a>
                    <a href="#">4</a>
                    <a href="#">5</a>
                    <a href="#">6</a>
                    <a href="#">&raquo;</a>
                </div>*@
                
            </div>
        </div>
        <div ng-if="AgentDetail.length==0 || AgentDetail == null">
            <img src="~/images/7486809.png" />
            <h2>
                No Record Found
            </h2>
        </div>

        <toaster-container toaster-options="{'toaster-id': 'notification', 'time-out':{ 'toast-warning': 1000, 'toast-error': 0 }}">
        </toaster-container>
        <toaster-container toaster-options="{'toaster-id': 'page-validation', 'position-class': 'toast-center', 'limit': 5}">
        </toaster-container>
        <script type='text/ng-template' id='validation-error-toast.html'>
            {{totalErrors}} Errors found
            <button class="toast-button" ng-click="more()">More</button>
        </script>
        <div ng-show="imgloader" class="preloader">
            <img src="~/images/loadernew.gif" ng-show="imgloader" />
        </div>
    </div>
</center>
