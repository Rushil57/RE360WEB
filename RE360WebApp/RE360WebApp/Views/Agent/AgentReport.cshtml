@{
    Layout = "~/Views/PartialView/Layout.cshtml";
}
<link href="~/css/stylereport.css" rel="stylesheet" />
@*<script src="~/js/AgentReport.js"></script>*@
<div class="main" ng-app="MyApp" ng-controller="AgentReportController" ng-init="init()">
    <div class="header">
        <a href="/Agent/AgentReport" class="logo">
            <img src="/images/logore360.png" style="height: 52px;">
        </a>
        <div class="header-right">
            <button ng-click="redirectRegi()" class="btnback">
                <div class="PrimaryDefault" style="flex: 1 1 0;text-align: center;color: white;font-size: 15px;font-family: sans-serif;font-weight: 700;line-height: 24px;word-wrap: break-word;padding: 0px 10px;">
                    Add New Agent
                </div>
            </button>
            <button ng-click="Logout()" class="btnback">
                <div class="PrimaryDefault" style="flex: 1 1 0; text-align: center; color: white; font-size: 15px; font-family: sans-serif; font-weight: 700; line-height: 24px; word-wrap: break-word;padding: 0px 10px;">
                    Log Out
                </div>
            </button>
        </div>
    </div>
    <div @*style="padding-left:20px"*@>
        <div class="mainDiv">
            <ul id="childWrapper">
                <li class="li1"><div><img src="~/images/re360main2.jpeg" /></div></li>
                <li class="li2">
                    <div style="overflow-y: scroll;max-height: 653px; ">
                        @* <center> *@
                        <div class="Dashboard" style="width: auto;height: auto;padding-bottom: 41px;background: white;flex-direction: column;justify-content: flex-start;align-items: center;gap: 40px;display: inline-flex;/* border: 1px solid black; */">
                                <div class="TitleRegister">
                                    <div class="Register"><span>Agent List</span> </div>
                                    <br>
                                </div>
                                <div class="ListOfClient" ng-if="AgentDetail.length> 0 && AgentDetail !=null ">
                                    <div class="ClientList" ng-repeat="item in AgentDetail | orderBy:'FirstName'">
                                        <div ng-class="{'ListView':$index%2==0,'ListView2':$index%2!=0}">
                                            <br />
                                            <div class="UserDetails">
                                                <div class="ClientDiv">
                                                    <div style="justify-content: flex-start; align-items: flex-start; gap: 4px; display: flex">
                                                        <div class="UserSolid">
                                                            <img src="~/images/user.svg" style="width: 24px; height: 24px;" />
                                                        </div>
                                                        <div style="width: 236px; flex-direction: column; justify-content: center; align-items: flex-start; gap: 4px; display: inline-flex">
                                                            <div class="CameronWilliamsonDavid" style="align-self: flex-start; color: #000000; font-size: 16px; font-family: sans-serif; font-weight: 500; line-height: 24px; word-wrap: break-word">
                                                                {{item.firstName}}
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div style="justify-content: flex-start; align-items: flex-start; gap: 4px; display: flex">
                                                        <div class="UserSolid" style="width: 24px; height: 24px; position: relative">
                                                            <img src="~/images/email_icon.svg" style="width: 24px; height: 24px;" />
                                                        </div>
                                                        <div class="CreateDate" style="width: 236px; flex-direction: column; justify-content: center; align-items: flex-start; gap: 4px; display: inline-flex">
                                                            <div class="July20231000Pm" style="align-self: flex-start; color: #000000; font-size: 16px; font-family: sans-serif; font-weight: 500; line-height: 24px; word-wrap: break-word">
                                                                {{item.email}}
                                                            </div>
                                                        </div>
                                                    </div>
                                                <button ng-click="redirectRegi(item.agentID)" class="SharePdf" style="padding: 8px; background: #E6E9ED; border-radius: 4px; overflow: hidden; justify-content: center; align-items: center; gap: 9px; display: inline-flex;width: 95px;border: none;cursor: grab;margin-left: 100px;">
                                                        <div class="Share2" style="width: 16px; height: 16px; position: relative;">
                                                            <img src="~/images/edit_notepad_icon.svg" style="width: 16px; height: 16px;" />
                                                        </div>
                                                        <div class="SharePdf" style="color: #000000; font-size: 16px; font-family: sans-serif; font-weight: 500; line-height: 16px; word-wrap: break-word">Edit</div>
                                                    </button>
                                                </div>
                                                <div class="ClientDiv">
                                                    <div style="justify-content: flex-start; align-items: flex-start; gap: 4px; display: flex;">
                                                        <div class="UserSolid">
                                                            <img src="~/images/companylogo.svg" style="width: 24px; height: 24px;" />
                                                        </div>
                                                        <div style="width: 236px; flex-direction: column; justify-content: center; align-items: flex-start; gap: 4px; display: inline-flex">
                                                            <div class="CameronWilliamsonDavid" style="align-self: flex-start; color: #878786; font-size: 16px; font-family: sans-serif; font-weight: 500; line-height: 24px; word-wrap: break-word">
                                                                {{item.companyName}}
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div style="justify-content: flex-start; align-items: flex-start; gap: 4px; display: flex">
                                                        <div class="UserSolid">
                                                            <img src="~/images/officelogo.svg" style="width: 24px; height: 24px;" />
                                                        </div>
                                                        <div class="CreateDate" style="width: 236px; flex-direction: column; justify-content: center; align-items: flex-start; gap: 4px; display: inline-flex">
                                                            <div class="July20231000Pm" style="align-self: flex-start; color: #878786; font-size: 16px; font-family: sans-serif; font-weight: 500; line-height: 24px; word-wrap: break-word">
                                                                {{item.offinceName}}
                                                            </div>
                                                        </div>
                                                    </div>
                                                <button ng-click="DeleteAgentByID(item.agentID)" class="deleteuser" style="padding-left: 10px; padding-right: 10px; padding-top: 8px; padding-bottom: 8px; background: #FCEBEC; border-radius: 4px; overflow: hidden; justify-content: center; align-items: center; gap: 9px; display: inline-flex;border: none;cursor: grab;;margin-left: 100px;">
                                                        <div class="FilePdf" style="width: 16px; height: 16px; position: relative">
                                                            <img src="~/images/red-delete.svg" style="width: 16px; height: 16px;" />
                                                        </div>
                                                        <div class="ViewPdf" style="color: #bc3c3c; font-size: 16px; font-family: sans-serif; font-weight: 500; line-height: 16px; word-wrap: break-word">Delete</div>
                                                    </button>
                                                </div>
                                            </div>
                                        </div>


                                    </div>
                                    <div>
                                    </div>
                                </div>
                                <div ng-if="AgentDetail.length==0 || AgentDetail ==null ">
                                    <img src="~/images/no property record found.png" style="height: 400px;" />
                                </div>

                               @*  <toaster-container toaster-options="{'toaster-id': 'notification', 'time-out':{ 'toast-warning': 1000, 'toast-error': 0 }}">
                                </toaster-container>
                                <toaster-container toaster-options="{'toaster-id': 'page-validation', 'position-class': 'toast-center', 'limit': 5}">
                                </toaster-container>
                                <script type='text/ng-template' id='validation-error-toast.html'>
                                    {{totalErrors}} Errors found
                                    <button class="toast-button" ng-click="more()">More</button>
                                </script> *@

                            </div>
                        @* </center> *@
                    </div>
                </li>
            </ul>
        </div>
    </div>
    <div ng-class="{'preloader':imgloader,'':imgloader}"> 
        <img src="~/images/loadernew.gif" ng-if="imgloader" />
    </div>
</div>

